            }
            else {
                event.preventDefault();
                $('#mainpage').addClass('hidden');
                let html = " <div  style='background-color:#f1f1f1; height:300px' >";
                html += "<div class='row'><br><div class='col-xs-12 col-sm-12 col-md-12 col-lg-12' ><div class='col-xs-3'></div><div class='col-xs-6' style='background-color:white;''>";
