                html += " <div class='col-xs-5'></div><button  onclick='sendsingup();' style='height: 25px; background: #f16e8e;font-weight: bold;'>Đăng Ký ! </button><br>";
                html += "<div class='col-xs-1'></div></div></div></div> </div><br></div><hr>";
                document.getElementById('login1').innerHTML = html;
            }
        }
        function sendsingup() {
            let phone = $('#phone').val();
            let pass = $('#pass').val();
            if (phone.length == 10 & pass.length > 5) {
                axios.post('/api/singup', {
                    phone: phone,
                    pass: pass
                }).then((res) => {
                    console.log(res.data);
                    if (res.data.code != 1000) {
                        alert("Thông tin bạn nhập không chính xác, vui lòng nhập lại.")
                        $('#phone').val("").focus();
                        $('#pass').val("");
                    }
                    else{
                        alert("Bạn đã đăng ký thành công. Mời đăng nhập để sử dụng dịch vụ.");
                        login();
                    }
                })
            }
            else {

                if (phone.length != 10) {
                    $('#phone').val("").focus();
                    $('#pass').val("");
                    alert("SDT không hợp lệ, vui lòng kiểm tra lại!");
                }
                if (pass.length < 6) {
                    $('#pass').val("").focus();
                    alert("Mật khẩu quá ngắn, vui lòng thử lại!");
                }

            }
        }
        function search() {
