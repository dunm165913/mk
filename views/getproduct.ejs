        function report1() {
            let a = $('#subject').val();
            let b = $('#details').val();
            axios.post('/api/report_products', {
                product_id: `<%- id.id %>`,
                subject: a,
                details: b
            }, {
                    headers: {
                        "Authorization": "Bearer " + localStorage.getItem('token')
                    }
                }).then((res) => {
                    if (res.data.code == 1000) {
                        console.log(res);
                        alert("Thành công");
                        var html = "" + res.data.data + "";
                        document.getElementById('rp').innerHTML = html;
                        $('#report').addClass('hidden');

                    }
                })
        }
        function singup() {
            if (localStorage.getItem("token") != null) {
                localStorage.clear();
