    <script>
        let user = {};
        var dlike;
        var dilike;
        var like = 0;
        var dislike = 0, dbc = 0;;
        function like1() {
            like++;
            if (like % 2 == 1) {
                if (localStorage.getItem('token') != null) {

                    $('#dislike').addClass('hidden');
                    axios.post('/api/like_products', {
                        id: `<%- id.id %>`,
                    }, {
                            headers: {
                                "Authorization": "Bearer " + localStorage.getItem('token')
                            }
                        }).then((res) => {
                            if (res.data.code != 1000) {
                                //console.log(res);
                                alert(res.data.message);
                            }
                            else {
                                alert("Bạn đã like thành công");
                                document.getElementById("like").style.color = "#ff789e";
                                dlike++;
                                console.log(dlike);
                                var html = "" + dlike + "";
                                document.getElementById('l').innerHTML = html;
                            }
                        })
                }
                else {
                    alert("Bạn phải đăng nhập để like sản phẩm");
                    login();
                }
            }

            else {
                if (localStorage.getItem('token') != null) {
                    document.getElementById("like").style.color = "gold";

                    $('#dislike').removeClass('hidden');
                    axios.post('/api/unlike', {
                        id: `<%- id.id %>`,
                    }, {
                            headers: {
                                "Authorization": "Bearer " + localStorage.getItem('token')
                            }
                        }).then((res) => {
                            if (res.data.code != 1000) {
                                //console.log(res);
                                alert(res.data.message);
                            }
                            else {
                                alert("Bạn đã unlike thành công");
                                document.getElementById("like").style.color = "gold";
                                dlike--;
                                console.log(dlike);
                                var html = "" + dlike + "";
                                document.getElementById('l').innerHTML = html;
                            }
                        })
                }
                else {
                    alert("Bạn phải đăng nhập để unlike sản phẩm")
                    login();
                }
            }
        }

        function dislike1() {
            dislike++;
